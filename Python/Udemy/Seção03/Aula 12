#Formatação de Strings F-STRINGS

altura, nome, peso = 1.75, 'Bruno Henrique', 72
imc = peso / altura ** 2

linha_1 = f'{nome} \ntem {altura} \nde altura e seu imc é {imc:.3f}'
print(linha_1)

# Função Format
 
a, b, c = 'A' , 'B', '1.154'
c = float(float(c))
string = 'a = {name1} b = {name2} c = {name3:.2f} c = {name3}'
formato = string.format(name1 = a, name2 = b, name3 = c) # Parametro Nomeado
print(formato)
